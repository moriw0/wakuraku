<%= link_to(event, class: 'list-group-item list-group-item-action') do %>
  <h2 class="list-group-item-heading"><%= event.name %> </h2>
  <p class="mb-1"><%= event.title %> </p>
  <ul>
    <% event.hosted_dates.each do |date| %>
      <li s class="mb-1"><%= format_date(date) %></li>
    <% end %>
  </ul>
<% end %>
